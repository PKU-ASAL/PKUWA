function %br_icmp_inversion(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
    br_icmp ugt v0, v1, block1
    jump block2
block1:
    v2 = iconst.i32 1
    return v2
block2:
    v3 = iconst.i32 2
    return v3
}
