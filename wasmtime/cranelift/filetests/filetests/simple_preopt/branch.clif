test simple_preopt
target aarch64
target x86_64

function %icmp_to_brz_fold(i32) -> i32 {
block0(v0: i32):
    v1 = icmp_imm eq v0, 0
    brnz v1, block1
    jump block2
block1:
    v3 = iconst.i32 1
    return v3
block2:
    v4 = iconst.i32 2
    return v4
}
