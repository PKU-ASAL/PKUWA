function %fmin_is_nan_f32(f32, f32) -> i32 {
block0(v0: f32, v1: f32):
    v2 = fmin_pseudo v0, v1
    v3 = fcmp ne v2, v2
    v4 = bint.i32 v3
    return v4
}
