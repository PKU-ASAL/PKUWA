test licm
target aarch64

function %rewrite_jump_table() {
    jt0 = jump_table [block1, block2]

    block0:
        v0 = iconst.i32 1
        br_table v0, block1, jt0

    block1:
        return

    block2:
        v4 = bconst.b1 false
        jump block2
}

